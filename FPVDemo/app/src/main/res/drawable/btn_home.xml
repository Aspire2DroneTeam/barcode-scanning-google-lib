<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape  android:shape="rectangle" >
            <corners
                android:radius="14dp"
                />
            <solid
                android:color="#522e1f"
                />
            <size
                android:width="79dp"
                android:height="26dp"
                />
            <stroke
                android:width="1dp"
                android:color="#522e1f"
                />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape  android:shape="rectangle" >
            <corners
                android:radius="14dp"
                />
            <solid
                android:color="#B56644"
                />
            <size
                android:width="79dp"
                android:height="26dp"
                />
            <stroke
                android:width="1dp"
                android:color="#B56644"
                />
        </shape>
    </item>
</selector>
