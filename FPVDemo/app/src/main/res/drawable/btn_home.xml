<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle" >
            <corners
                android:radius="14dp"
                />
            <solid
                android:color="@color/colorBtnHome"
                />
            <size
                android:width="79dp"
                android:height="26dp"
                />
            <stroke
                android:width="1dp"
                android:color="@color/colorBtnHome"
                />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="14dp" />
            <solid android:color="@color/colorBtnHomePressed" />
            <size android:width="79dp" android:height="26dp" />
            <stroke android:width="1dp" android:color="@color/colorBtnHomePressed" />
        </shape>
    </item>
</selector>
